<?php

namespace eXmarket;

class MarketSellOrder
{
	private $database_id;
	private $eve_id;
	private $volume;
	private $min_volume;
	private $station_id;
	private $duration;
	private $price;
	private $date_issued;
	private $range;
	
	function __construct($database_id,$eve_id,$volume,$min_volume,$station_id,$duration,$price,$date_issued,$range)
	{
		$this->database_id = $database_id;
		$this->eve_id = $eve_id;
		$this->volume = $volume;
		$this->min_volume = $min_volume;
		$this->station_id = $station_id;
		$this->duration = $duration;
		$this->price = $price;
		$this->date_issued = $date_issued;
		$this->range = $range;
	}
}




?>