<?php

namespace eXmarket;

class MarketGroup
{
	private $database_id;
	private $eve_id;
	private $name;
	private $description;
	
	public function __construct($database_id, $eve_id,$name,$description)
	{
		$this->database_id = null;
		$this->eve_id = $eve_id;
		$this->name = $name;
		$this->description = $description;
	}
}




?>